{
  "name": "@pavelg/grpc",
  "version": "0.0.1-alpha.18",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@grpc/grpc-js": "^1.1.0",
    "@grpc/proto-loader": "^0.6.1",
    "@types/long": "^4.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^12.12.0",
    "jest": "^26.6.3",
    "node-gyp": "^8.0.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.2"
  },
  "scripts": {
    "compile": "tsc -b",
    "build:proto": "proto-loader-gen-types --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --includeDirs=protos --outDir=src/types local_service.proto",
    "precompile": "npm run build:proto",
    "build:server": "tar -xzvf zetasql/darwin-x86-64/libremote_server.dylib.tar.gz -C zetasql/darwin-x86-64/ && tar -xzvf zetasql/linux-x86-64/libremote_server.so.tar.gz -C zetasql/linux-x86-64/ && node-gyp configure && node-gyp rebuild -v",
    "build:all": "npm run build:server && npm run format && npm run compile",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json"
  },
  "files": [
    "package.json",
    "binding.gyp",
    "server.cc",
    "zetasql/**/*",
    "lib/**/*",
    "protos/**/*"
  ]
}
