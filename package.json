{
  "name": "@fivetrandevelopers/zetasql",
  "displayName": "ZetaSQL NPM",
  "description": "ZetaSQL - Analyzer Framework for SQL available via NPM",
  "version": "1.0.1",
  "publisher": "Fivetran",
  "license": "MIT",
  "preview": true,
  "keywords": [
    "npm",
    "bigquery",
    "sql"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@grpc/grpc-js": "^1.4.2",
    "@grpc/proto-loader": "^0.6.6",
    "@types/long": "^4.0.1",
    "ffi-napi": "^4.0.3"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "compile": "tsc -b",
    "build:proto": "proto-loader-gen-types --defaults --oneofs --grpcLib=@grpc/grpc-js --includeDirs=src/protos --outDir=src/types local_service.proto",
    "precompile": "npm run build:proto",
    "build:server": "mkdir -p lib/zetasql && tar -xzvf zetasql/darwin-x86-64/libremote_server.dylib.tar.gz -C lib/zetasql/ && tar -xzvf zetasql/linux-x86-64/libremote_server.so.tar.gz -C lib/zetasql/ && cp -R src/protos/ lib/protos/",
    "build:all": "npm run build:server && npm run lint && npm run compile",
    "test": "jest --config jestconfig.json",
    "lint": "eslint . --ext .ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package.json",
    "lib/**/*"
  ]
}
