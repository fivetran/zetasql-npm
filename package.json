{
  "name": "@fivetrandevelopers/zetasql",
  "version": "1.0.0-alpha.12",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@grpc/grpc-js": "^1.3.2",
    "@grpc/proto-loader": "^0.6.2",
    "@types/long": "^4.0.1",
    "ffi-napi": "^4.0.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^12.20.13",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.2"
  },
  "scripts": {
    "compile": "tsc -b",
    "build:proto": "proto-loader-gen-types --defaults --oneofs --grpcLib=@grpc/grpc-js --includeDirs=protos --outDir=src/types local_service.proto",
    "precompile": "npm run build:proto",
    "build:server": "tar -xzvf zetasql/darwin-x86-64/libremote_server.dylib.tar.gz -C zetasql/ && tar -xzvf zetasql/linux-x86-64/libremote_server.so.tar.gz -C zetasql/",
    "build:all": "npm run build:server && npm run format && npm run compile",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package.json",
    "zetasql/libremote_server.dylib",
    "zetasql/libremote_server.so",
    "lib/**/*",
    "protos/**/*"
  ]
}
