{
  "typescript.preferences.quoteStyle": "single",

  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,

  "mochaExplorer.files": "lib/test/**/*.js",
  "mochaExplorer.require": "source-map-support/register",
  "mochaExplorer.watch": "lib/**/*.js",

  "cSpell.words": [
    "abhinavsingh",
    "bazel",
    "BIGNUMERIC",
    "cond",
    "datetime",
    "deserialization",
    "dylib",
    "eqeqeq",
    "Fivetran",
    "fivetrandevelopers",
    "hamjest",
    "libremote",
    "mkdir",
    "mockito",
    "napi",
    "NULLIFERROR",
    "oneofs",
    "Parens",
    "prebuild",
    "proto",
    "protobuf",
    "protos",
    "resolvedast",
    "sonarjs",
    "tokenlist",
    "UNPIVOT",
    "Unprepare",
    "xzvf",
    "zetasql"
  ]
}
