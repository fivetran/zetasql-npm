{
  "name": "@fivetrandevelopers/zetasql",
  "description": "ZetaSQL gRPC server and client",
  "version": "1.1.4",
  "publisher": "Fivetran",
  "license": "MIT",
  "keywords": [
    "ZetaSQL",
    "BigQuery",
    "SQL",
    "gRPC"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/fivetran/zetasql-npm.git"
  },
  "homepage": "https://github.com/fivetran/zetasql-npm#readme",
  "dependencies": {
    "@grpc/grpc-js": "^1.4.5",
    "@grpc/proto-loader": "^0.6.7",
    "@types/long": "^4.0.1",
    "ffi-napi": "^4.0.3"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.3",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "build": "tsc -b",
    "watch": "tsc -b -w",
    "build:proto": "proto-loader-gen-types --defaults --oneofs --grpcLib=@grpc/grpc-js --includeDirs=src/protos --outDir=src/types local_service.proto",
    "prebuild": "npm run build:proto",
    "build:server": "mkdir -p lib/zetasql && tar -xzvf zetasql/darwin-x86-64/libremote_server.dylib.tar.gz -C lib/zetasql/ && tar -xzvf zetasql/linux-x86-64/libremote_server.so.tar.gz -C lib/zetasql/ && cp -R src/protos/ lib/protos/",
    "build:all": "npm run build:server && npm run lint && npm run build",
    "test": "jest --config jestconfig.json",
    "lint": "eslint --cache --no-fix --ext .ts src",
    "lint:fix": "eslint --cache --fix --ext .ts src",
    "prepare": "husky install"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package.json",
    "lib/**/*",
    "!lib/TestZetaSqlServer*"
  ],
  "lint-staged": {
    "*.ts": [
      "eslint --cache --ext .ts --fix src"
    ]
  }
}
